{
    "name": "RPG Extension",
    "version": "0.1.0",
    "description": "Decodificar alfabeto",
    "permissions": ["storage"],
    "host_permissions": ["https://*"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*"],
            "js": ["contentScript.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/x.png",
                "assets/x.png",
                "assets/x.png",
                "assets/x.png"
            ],
            "matches": ["https://*"]
        }
    ],
    "action": {
        "default_icon": {
            "16": "assets/y.png",
            "24": "assets/y.png",
            "32": "assets/y.png"
        },
        "default_title": "RPG Extension",
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}